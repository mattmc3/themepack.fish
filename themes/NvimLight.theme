# name: "NvimLight"
# preferred_background: e0e2ea

# color-palette {
#  background: #e0e2ea;
#  foreground: #14161b;
#  selection:  #9b9ea4;
#  cursor:     #9b9ea4;
#
#   black:     #07080d;
#   red:       #590008;
#   green:     #005523;
#   yellow:    #6b5300;
#   blue:      #004c73;
#   magenta:   #470045;
#   cyan:      #007373;
#   white:     #eef1f8;
#
#   brblack:   #4f5258;
#   brred:     #590008;
#   brgreen:   #005523;
#   bryellow:  #6b5300;
#   brblue:    #004c73;
#   brmagenta: #470045;
